{% extends 'base.html.twig' %}

{% block title %}Fan Webapp | Orders{% endblock %}

{% block body %}
    {% if products is not null %}
        <ul>
            {% for product in products %}
                <li>
                    <p>{{ product.label }}</p>
                </li>
            {% endfor %}
        </ul>
    {% else %}
    <h1>Orders</h1>
    {% endif %}
{% endblock %}
